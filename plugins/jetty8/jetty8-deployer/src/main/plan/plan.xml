                    </init-param>
                    <init-param>
                        <param-name>minGzipLength</param-name>
                        <param-value>8192</param-value>
                    </init-param>
                    <load-on-startup>0</load-on-startup>
                </servlet>
                <servlet-mapping>
                    <servlet-name>default</servlet-name>
                    <url-pattern>/</url-pattern>
                </servlet-mapping>

